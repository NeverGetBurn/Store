<!DOCTYPE html>
<html>
<head>
    <!-- layout head -->
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="shortcut icon" href="/favicon.ico" />
    <!-- layout head -->
    <title>@ViewBag.Title</title>
    <link src="lib/bootstrap/dist/css/bootstrap.min.css" />
    <link src="css/site.css" />    
</head>
<body>
    <div class="alert-primary">
        <nav class="navbar justify-content-center nav-pills">
            <a class="btn btn-outline-primary nav-item nav-link" href="~/Home/">Домашняя страница</a>
            <a class="ml-1 btn btn-outline-primary nav-item nav-link" href="~/Storage/">Каталог</a>
        </nav>
    </div>
    <!-- layout render body -->
    @RenderBody()

    <!-- / layout render body -->
    <script src="lib/jquery/dist/jquery.min.js"></script>
    <script src="lib/bootstrap/dist/js/bootstrap.min.js"></script>
    @RenderSection("scripts", false)
</body>
</html>
